{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@dtos/*": ["./src/dtos/*"],
      "@server/*": ["./src/server/*"],
      "@pages/*": ["./src/client/pages/*"],
      "@components/*": ["./src/client/components/*"],
      "@hooks/*": ["./src/client/hooks/*"],
      "@/*": ["./src/client/*"],
      "@@/*": ["./src/client/.umi/*"]
    }
  }
}
